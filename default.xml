<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote fetch="http://code.ossystems.com.br" name="code" review="code.ossystems.com.br"/>
  <remote fetch="git://git.yoctoproject.org" name="yocto"/>
  <remote fetch="git://github.com/Freescale" name="fslc"/>
  <remote fetch="git://git.openembedded.org" name="oe"/>

  <default revision="invalidRevision" sync-j="4"/>

  <project remote="yocto" revision="dora" name="poky" path="sources/poky"/>
  <project remote="oe"    revision="master" name="meta-openembedded" path="sources/meta-openembedded"/>

  <project remote="code"  revision="dora" name="ossystems-yocto-base-scripts" path="sources/base">
    <copyfile dest="setup-environment" src="setup-environment"/>
  </project>

  <!-- O.S. Systems Yocto BSP -->
  <project remote="code" revision="dora" name="meta-ossystems-base" path="sources/meta-ossystems-base"/>
  <project remote="code" revision="dora" name="meta-ossystems" path="sources/meta-ossystems"/>
  <project remote="code" revision="dora" name="meta-fsl-arm" path="sources/meta-fsl-arm"/>
  <project remote="code" revision="dora" name="meta-fsl-arm-extra" path="sources/meta-fsl-arm-extra"/>
  <project remote="code" revision="dora" name="meta-fsl-demos" path="sources/meta-fsl-demos"/>

  <!-- conf -->
  <project remote="code" revision="dora" name="ossystems-yocto-platform" path="sources/conf/repo"/>
  <project remote="code" revision="dora" name="ossystems-yocto-config" path="sources/conf/tools"/>

</manifest>
